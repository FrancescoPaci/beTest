<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <context id="MySqlContext" targetRuntime="MyBatis3" defaultModelType="flat">

        <plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin" />
        <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin" />

        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
            <property name="suppressDate" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://db4free.net:3306/frankpax_db?useSSL=false"
                        userId="frankpax"
                        password="FrankoPax$db12">
            <property name="nullCatalogMeansCurrent" value="true" />

        </jdbcConnection>

        <javaModelGenerator targetPackage="pax.test.mybatis.model" targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="pax.test.mybatis.mapper"  targetProject="src/main/resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="pax.test.mybatis.mapper" targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

        <!--<table tableName="%"></table>-->

        <table tableName="Orders">
            <columnOverride column="Order_Date" jdbcType="DATE" typeHandler="org.apache.ibatis.type.SqlDateTypeHandler"/>
            <columnOverride column="required_Date" jdbcType="DATE" typeHandler="org.apache.ibatis.type.SqlDateTypeHandler"/>
            <columnOverride column="shipped_Date" jdbcType="DATE" typeHandler="org.apache.ibatis.type.SqlDateTypeHandler"/>
        </table>
        <table tableName="Customers"></table>
        <table tableName="Orders_Details"></table>
        <table tableName="Products"></table>
        <table tableName="Shippers"></table>
        <table tableName="Users"></table>

        <table tableName="ALLTYPES" >
            <columnOverride column="UNSIGNED_BIGINT_FIELD" javaType="java.lang.Object" jdbcType="LONG" />
        </table>

    </context>

</generatorConfiguration>